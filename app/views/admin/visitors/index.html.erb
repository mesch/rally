<div id="bdw" class="bdw">
<div id="bd" class="cf">
<div>
    <div id="content" class="clear mainwide">
		<div class="box clear">
            <div class="box-top"></div>
            <div class="box-content">
                <div class="head">
                    <h2>Visitors</h2>
				</div>
                <div class="sect">
					<table id="list" cellspacing="0" cellpadding="0" border="0" class="general-table">
						<tr><th width="40">Id</th>
							<th width="60">Created</th>
						</tr>							
						<% for visitor in @visitors %>
						<tr>
							<td><%= visitor.id %></td>
							<td><%= visitor.created_at.strftime(OPTIONS[:date_format]) %></td>
						</tr>
						<% end %>
                    </table>
				</div>
            </div>
            <div class="box-bottom"></div>
        </div>
    </div>
</div>
</div> <!-- bd end -->
</div> <!-- bdw end -->